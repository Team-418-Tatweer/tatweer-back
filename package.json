{
    "name": "vinos",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "type": "commonjs",
    "scripts": {
        "build": "npx tsc",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/dev.js\"",
        "doc": "npx typedoc --entryPointStrategy expand ./src",
        "seed": "npx tsc && node ./dist/db/seeds/index.js",
        "start": "node dist/index.js",
        "test": "cross-env NODE_ENV=test BACK_MONGODB_NAME=testDb jest  --setupFiles dotenv/config --no-cache   --detectOpenHandles --forceExit",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "swagger": "ts-node ./src/docs/swagger.ts",
        "format": "prettier --write .",
        "ts": "tsc --noEmit --incremental"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "date-fns": "^2.30.0",
        "discord.js": "^14.16.3",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.1",
        "module-alias": "^2.2.3",
        "mongodb": "^6.9.0",
        "mongoose": "^7.4.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.4",
        "supertest": "^6.3.3",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.0",
        "winston": "^3.11.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@nrwl/jest": "^19.8.4",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.13",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/module-alias": "^2.0.4",
        "@types/mongodb": "^4.0.6",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.4.2",
        "@types/nodemailer": "^6.4.8",
        "@types/supertest": "^2.0.16",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "concurrently": "^8.2.1",
        "dotenv": "^16.4.4",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typedoc": "^0.24.8",
        "typescript": "^5.1.6"
    },
    "_moduleAliases": {
        "@modules": "dist/modules",
        "@services": "dist/services",
        "@utils": "dist/utils",
        "@db": "dist/db",
        "@routes": "dist/routes",
        "@middleware": "dist/middleware",
        "@config": "dist/config",
        "@myTypes": "dist/types",
        "@controllers": "dist/controllers",
        "@models": "dist/db/models"
    },
